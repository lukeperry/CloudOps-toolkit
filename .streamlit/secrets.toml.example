# Streamlit secrets for AWS credentials
# Copy this file to secrets.toml and add your actual AWS credentials

[aws]
AWS_ACCESS_KEY_ID = "your-access-key-id"
AWS_SECRET_ACCESS_KEY = "your-secret-access-key"
AWS_REGION = "ap-southeast-1"

[general]
debug = false
environment = "production"

# Optional: Add other service configurations here
[terraform]
auto_apply = false
backend_bucket = "your-terraform-state-bucket"
